{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/components/*": ["src/components/*"],
      "@/containers/*": ["src/containers/*"],
      "@/hooks/*": ["src/hooks/*"],
      "@/js/*": ["src/js/*"],
      "@/styles/*": ["styles/*"],
      "@/resources/*": ["resources/*"]
    }
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
